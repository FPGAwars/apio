      "name": "tools-drivers",
      "organization": "FPGAwars"
    },
    "release": {
      "tag_name": "v%V",
      "compressed_name": "tools-drivers-%P-%V",
      "uncompressed_name": "",
      "folder_name": "tools-drivers",
      "extension": "tar.gz",
      "url_version": "https://github.com/FPGAwars/tools-drivers/raw/master/VERSION",
